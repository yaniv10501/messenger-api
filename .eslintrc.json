{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "no-underscore-dangle": [
        "error",
        {
          "allow": ["_id", "_find"]
        }
      ]
    }
  }
